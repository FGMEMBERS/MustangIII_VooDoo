<?xml version="1.0"?>
<PropertyList>

 <path>MustangIII_VooDoo-trimwheelelevator.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>KnobTop</object-name>
  <object-name>Knob</object-name>
  <property>controls/flight/elevator-trim</property>
  <factor>1440</factor>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle</object-name>
  <property>controls/flight/elevator-trim</property>
  <interpolation>
   <entry>
    <ind>-1.0</ind>
    <dep>-50</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.0</ind>
    <dep>50</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


